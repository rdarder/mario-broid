{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "interface-name": false,
    "quotemark": false,
    "semicolon": false,
    "member-access": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "max-classes-per-file": [
      false,
      1
    ],
    "object-literal-sort-keys": false,
    "no-object-literal-type-assertion": false,
    "no-namespace": false,
    "no-reference": false
  }
}
